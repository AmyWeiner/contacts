<h2>Contacts</h2>

<table class='table table-striped'>
<th>First Name</th>
<th>Last Name</th>
<th>Email</th>
<th>Phone Number</th>
<% @people.each do |person| %>
  <tr>
    <td><%= person.first_name %></td>
    <td><%= person.last_name %></td>
    <td><%= person.email %></td>
    <td><%= person.phone %></td>
  </tr>  
<% end %> 
</table>
